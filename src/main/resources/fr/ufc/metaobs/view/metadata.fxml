<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<BorderPane xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx/">
    <top>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="General metadata" BorderPane.alignment="CENTER">
            <font>
                <Font size="18.0"/>
            </font>
        </Text>
    </top>
    <center>
        <GridPane fx:id="metadataGridPane" hgap="10" vgap="20" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets top="10.0"/>
            </BorderPane.margin>
        </GridPane>
    </center>
</BorderPane>
