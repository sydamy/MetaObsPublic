<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<BorderPane xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx">
    <top>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="List of observations" BorderPane.alignment="CENTER">
            <font>
                <Font size="18.0"/>
            </font>
        </Text>
    </top>
    <center>
        <TableView fx:id="observationsTableView" tableMenuButtonVisible="true" BorderPane.alignment="CENTER">
            <columns>
                <TableColumn fx:id="entityNameTableColumn" editable="false" prefWidth="-1.0" text="Entity name"/>
                <TableColumn fx:id="nameTableColumn" editable="false" prefWidth="-1.0" text="Observation name"/>
                <TableColumn fx:id="multiplicityTableColumn" editable="false" prefWidth="-1.0" text="Multiplicity"/>
                <TableColumn fx:id="actorTableColumn" editable="false" prefWidth="-1.0" text="Actor"/>
                <TableColumn fx:id="actorPropertiesTableColumn" editable="false" prefWidth="-1.0"
                             text="Actor properties"/>
                <TableColumn fx:id="locationTableColumn" editable="false" prefWidth="-1.0" text="Location"/>
                <TableColumn fx:id="locationPropertiesTableColumn" editable="false" prefWidth="-1.0"
                             text="Location properties"/>
                <TableColumn fx:id="dateTableColumn" editable="false" prefWidth="-1.0" text="Date"/>
                <TableColumn fx:id="dateFormatTableColumn" editable="false" prefWidth="-1.0" text="Date format"/>
                <TableColumn fx:id="dateStandardTableColumn" editable="false" prefWidth="-1.0" text="Date standard"/>
                <TableColumn fx:id="characteristicsTableColumn" editable="false" prefWidth="-1.0"
                             text="Characteristics"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </center>
</BorderPane>
