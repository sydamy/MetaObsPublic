<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Text?>
<BorderPane xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx">
    <top>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="List of entitity contexts" BorderPane.alignment="CENTER"/>
    </top>
    <center>
        <TableView fx:id="entityContextsTableView" tableMenuButtonVisible="true" BorderPane.alignment="CENTER">
            <columns>
                <TableColumn fx:id="entityTableColumn" prefWidth="-1.0" text="Entity"/>
                <TableColumn fx:id="actorTableColumn" prefWidth="-1.0" text="Actor"/>
                <TableColumn fx:id="actorPropertiesTableColumn" prefWidth="-1.0" text="Actor properties"/>
                <TableColumn fx:id="locationTableColumn" prefWidth="-1.0" text="Location"/>
                <TableColumn fx:id="locationPropertiesTableColumn" prefWidth="-1.0" text="Location properties"/>
                <TableColumn fx:id="dateTableColumn" prefWidth="-1.0" text="Date"/>
                <TableColumn fx:id="dateFormatTableColumn" prefWidth="-1.0" text="Date format"/>
                <TableColumn fx:id="datePropertiesTableColumn" prefWidth="-1.0" text="Date properties"/>
                <TableColumn fx:id="dateStandardTableColumn" prefWidth="-1.0" text="Date standard"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </center>
</BorderPane>
